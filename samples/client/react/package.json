{
  "name": "react-a2ui",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "echo 'Please specify a project: npm run dev:contact, npm run dev:restaurant, etc.'",
    "dev:contact": "npm run dev -w @a2ui/sample-contact-react",
    "dev:restaurant": "npm run dev -w @a2ui/sample-restaurant-react",
    "dev:rizzcharts": "npm run dev -w @a2ui/sample-rizzcharts-react",
    "dev:orchestrator": "npm run dev -w @a2ui/sample-orchestrator-react",
    "dev:gallery": "npm run dev -w @a2ui/sample-gallery-react",
    "build": "npm run build --workspaces --if-present",
    "build:contact": "npm run build -w @a2ui/sample-contact-react",
    "build:restaurant": "npm run build -w @a2ui/sample-restaurant-react",
    "build:rizzcharts": "npm run build -w @a2ui/sample-rizzcharts-react",
    "build:orchestrator": "npm run build -w @a2ui/sample-orchestrator-react",
    "build:gallery": "npm run build -w @a2ui/sample-gallery-react",
    "build:a2a-chat-canvas": "npm run build -w @a2ui/a2a-chat-canvas-react",
    "build:lit": "cd ../../../renderers/lit && npm i && npm run build",
    "build:react-renderer": "cd ../../../renderers/react && npm i && npm run build",
    "serve:agent:contact": "cd ../../agent/adk/contact_lookup && uv run .",
    "serve:agent:restaurant": "cd ../../agent/adk/restaurant_finder && uv run .",
    "demo:contact": "npm run build:lit && npm run build:react-renderer && concurrently -k -n \"AGENT,WEB\" -c \"magenta,blue\" \"npm run serve:agent:contact\" \"npm run dev:contact\"",
    "demo:restaurant": "npm run build:lit && npm run build:react-renderer && concurrently -k -n \"AGENT,WEB\" -c \"magenta,blue\" \"npm run serve:agent:restaurant\" \"npm run dev:restaurant\""
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true
  },
  "dependencies": {
    "@a2a-js/sdk": "^0.3.4",
    "@a2ui/lit": "file:../../../renderers/lit",
    "@a2ui/react": "file:../../../renderers/react",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "signal-utils": "^0.21.1"
  },
  "devDependencies": {
    "@rollup/rollup-darwin-arm64": "^4.53.5",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^4.2.0",
    "concurrently": "^9.2.1",
    "dotenv": "^16.4.0",
    "typescript": "^5.8.3",
    "uuid": "^11.1.0",
    "vite": "^7.3.0"
  },
  "workspaces": [
    "projects/*",
    "../../../renderers/lit",
    "../../../renderers/react"
  ]
}
