    "test:coverage": "jest --coverage",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/react": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^8.51.0",
