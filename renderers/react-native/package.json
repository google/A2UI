    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.7.0",
    "react": "18.2.0",
    "react-native": "0.73.0",
    "ts-jest": "^29.4.6",
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-native": ">=0.70.0"
  },
