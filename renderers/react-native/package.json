  "homepage": "https://github.com/sivamrudram-eng/a2ui-react-native#readme",
  "bugs": {
    "url": "https://github.com/sivamrudram-eng/a2ui-react-native/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sivamrudram-eng/a2ui-react-native.git"
  },
  "license": "Apache-2.0",
  "author": "Barathwaj Ramamoorthy <rbarathwaj@gmail.com>",
  "type": "module",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "react-native": "src/index.ts",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "react-native": "./src/index.ts",
      "default": "./lib/index.js"
    }
  },
  "types": "lib/index.d.ts",
  "files": [
    "src",
    "lib"
  ],
  "scripts": {
    "build": "tsc",
