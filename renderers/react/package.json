{
  "name": "@a2ui/react",
  "version": "0.8.1",
  "description": "React implementation of A2UI - Declarative UI specification for AI agents",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:lib": "tsup",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "prepack": "npm run build:lib"
  },
  "keywords": [
    "a2ui",
    "react",
    "ui",
    "declarative",
    "ai",
    "agent",
    "components",
    "llm",
    "agentic"
  ],
  "author": "Google (specification), Community (React implementation)",
  "license": "Apache-2.0",
  "repository": {
    "directory": "renderers/react",
    "type": "git",
    "url": "git+https://github.com/google/A2UI.git"
  },
  "homepage": "https://github.com/google/A2UI/tree/main/renderers/react#readme",
  "bugs": {
    "url": "https://github.com/google/A2UI/issues"
  },
  "peerDependencies": {
    "react": "19.2.3",
    "react-dom": "19.2.3"
  },
  "dependencies": {
    "react-markdown": "10.1.0"
  },
  "devDependencies": {
    "@eslint/js": "9.39.2",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.1",
    "@types/node": "25.0.3",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "@vitejs/plugin-react": "5.1.2",
    "@vitest/coverage-v8": "4.0.16",
    "ajv": "8.17.1",
    "eslint": "9.39.2",
    "eslint-plugin-react-hooks": "7.0.1",
    "eslint-plugin-react-refresh": "0.4.26",
    "globals": "16.5.0",
    "jsdom": "27.3.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "tsup": "8.5.1",
    "typescript": "5.9.3",
    "typescript-eslint": "8.50.0",
    "vite": "7.3.0",
    "vitest": "4.0.16"
  }
}
