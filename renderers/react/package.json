{
  "name": "@a2ui/react",
  "version": "0.8.1",
  "description": "A2UI React Renderer - Full protocol implementation with streaming support",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "npm run build",
    "build": "wireit",
    "build:tsc": "wireit",
    "build:lib": "tsup",
    "dev": "vite",
    "test": "wireit",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint .",
    "copy-spec": "wireit"
  },
  "wireit": {
    "copy-spec": {
      "command": "mkdir -p src/schemas && cp ../../specification/0.8/json/*.json src/schemas",
      "files": [
        "../../specification/0.8/json/*.json"
      ],
      "output": [
        "src/schemas/*.json"
      ]
    },
    "test": {
      "command": "vitest run",
      "dependencies": [
        "copy-spec"
      ]
    },
    "build": {
      "dependencies": [
        "build:tsc"
      ]
    },
    "build:tsc": {
      "command": "tsup",
      "env": {
        "FORCE_COLOR": "1"
      },
      "dependencies": [
        "copy-spec"
      ],
      "files": [
        "src/**/*.ts",
        "src/**/*.tsx",
        "src/**/*.json",
        "tsconfig.json"
      ],
      "output": [
        "dist/"
      ],
      "clean": "if-file-deleted"
    }
  },
  "repository": {
    "directory": "renderers/react",
    "type": "git",
    "url": "git+https://github.com/google/A2UI.git"
  },
  "keywords": [
    "a2ui",
    "react",
    "ui",
    "declarative",
    "ai",
    "agent",
    "components",
    "llm",
    "streaming"
  ],
  "author": "Google",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/google/A2UI/issues"
  },
  "homepage": "https://github.com/google/A2UI/tree/main/renderers/react#readme",
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "dependencies": {
    "react-markdown": "^10.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "^4.0.16",
    "ajv": "^8.17.1",
    "eslint": "^9.39.2",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.26",
    "globals": "^16.5.0",
    "jsdom": "^27.3.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.0",
    "vite": "^7.3.0",
    "vitest": "^4.0.16",
    "wireit": "^0.15.0-pre.2"
  }
}
