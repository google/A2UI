{
  "name": "@a2ui/visual-parity",
  "version": "0.8.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev:react": "vite --config react/vite.config.ts --port 5001",
    "dev:lit": "vite --config lit/vite.config.ts --port 5002",
    "dev": "concurrently \"npm run dev:react\" \"npm run dev:lit\"",
    "build:react": "vite build --config react/vite.config.ts",
    "build:lit": "vite build --config lit/vite.config.ts",
    "test": "playwright test",
    "test:update": "playwright test --update-snapshots",
    "test:ui": "playwright test --ui",
    "test:report": "playwright show-report"
  },
  "dependencies": {
    "@a2ui/lit": "file:../../lit",
    "@a2ui/react": "file:..",
    "@lit/context": "^1.1.0",
    "lit": "^3.1.0",
    "markdown-it": "^14.0.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.1",
    "@types/pngjs": "^6.0.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^5.1.3",
    "concurrently": "^9.2.1",
    "pixelmatch": "^7.1.0",
    "pngjs": "^7.0.0",
    "typescript": "^5.8.0",
    "vite": "^7.3.1"
  }
}
